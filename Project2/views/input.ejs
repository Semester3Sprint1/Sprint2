<html lang="en">
  <%- include('./partials/header.ejs') %>
  <body>
    <%- include('./partials/nav.ejs') %>
    <div class="content">
      <h2>
        Enter a comma separated list of numbers, then you will receive a
        balanced, binary search Tree!!
      </h2>
      <form action="/proc" method="post">
        <label for="values"></label>
        <input
          type="text"
          name="values"
          placeholder="1,2,3,4,5, etc"
          id="values"
          required
        />
        <input type="submit" value="Treeify Me!" />
        <br />
      </form>
    </div>
  </body>
  <%- include('./partials/footer.ejs') %>
</html>
